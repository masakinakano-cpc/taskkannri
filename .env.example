# Google Calendar API 設定
# Google Cloud Console (https://console.cloud.google.com/) で取得してください

# OAuth 2.0 クライアントID
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# OAuth 2.0 クライアントシークレット
REACT_APP_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# OAuth リダイレクトURI
# 開発環境: http://localhost:3000/oauth/callback
# 本番環境: https://yourdomain.com/oauth/callback
REACT_APP_GOOGLE_REDIRECT_URI=http://localhost:3000/oauth/callback

# Base44 API設定（既存の設定がある場合）
# REACT_APP_BASE44_API_KEY=your_base44_api_key
# REACT_APP_BASE44_PROJECT_ID=your_base44_project_id
